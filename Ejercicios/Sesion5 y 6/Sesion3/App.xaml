<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:SampleData="clr-namespace:Expression.Blend.SampleData.MainDataSource"
             xmlns:ViewModels="clr-namespace:Sesion3.ViewModels"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="Sesion3.App"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
    			<ResourceDictionary Source="ResourceDictionary.xaml"/>
    		</ResourceDictionary.MergedDictionaries>
         
    		<SampleData:MainDataSource x:Key="MainDataSource" 
    			d:IsDataSource="True"/>
        
    		<ViewModels:MainViewModel x:Key="Container" />
         
    	</ResourceDictionary>
         
    </Application.Resources>
</Application>
